@{
    ViewData["Title"] = "Trail Info Page";
}
@foreach(var trail in ViewBag.Trail)
{
    <h1>@ViewBag.Trail.name</h1>
    <h3>Description</h3>
    <p>@ViewBag.Trail.Description</p>
    <h3>Trail Length</h3>
    <p>@ViewBag.Trail.Length</p>
    <h3>Elevation Change</h3>
    <p>@ViewBag.Trail.Elevation</p>
    <img src="">
    <p>Longitude - @ViewBag.Trail.Longitude</p>
    <p>Latitude - @ViewBag.Trail.Latitude</p>
}